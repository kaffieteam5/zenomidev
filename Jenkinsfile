pipeline{
    agent any
    stages{
        stage('1-start'){
            steps{
                echo "welcome to etech"
                sh 'lscpu'
            }
        }
        stage('2-keep working'){
            steps{
                echo "we are great engineers"
                sh 'cat/etc/passwd'
            }
        }
        stage('3-keep moving'){
            steps{
                echo "i must succed in devops"
                sh 'whoami'
            }
        }
    
    }
}